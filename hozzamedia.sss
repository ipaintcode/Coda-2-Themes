comment {
  color:#c1c1c1;
  font-style:italic;
}

constant.numeric {
  color:#f80000;
}

constant.numeric.keyword {
  color:#000088;
  font-weight:bold;
}

keyword {
  color:#000000;
  font-weight:bold;
}

keyword.control {
  color:#000088;
  font-weight:bold;
}

keyword.type {
  color:#000000;
