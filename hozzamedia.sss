}

language.function {
  color:#870088;
  font-style:italic;
}

language.operator {
  color:#000000;
  font-underline:none;
  font-weight:bold;
}

language.property {
  color:#0045aa;
}

language.variable {
  color:#438498;
}

markup.comment {
  color:#c1c1c1;
}

markup.constant.entity {
  color:#000000;
  font-weight:bold;
}

markup.declaration {
  color:#878787;
}

markup.inline.cdata {
  color:#bb2500;
}

markup.processing {
  color:#dd0000;
  font-style:italic;
  font-weight:bold;
}

markup.tag {
  color:#1a8887;
  font-weight:normal;
}

markup.tag.attribute.name {
  color:#000088;
  font-style:italic;
  font-weight:normal;
}

markup.tag.attribute.value {
  color:#000000;
